$(document).ready(function(){
    var count1 = 0;
    $('#wow_icon').click(function(){
        count2++;
        if (count2==5){
            $('#wow').html(
                "Live for the Wow<br><img src='image/doge.png' style='width:100px'>"
            );
        }
    });
    var count2=0;
    $('.rule2').click(function(){
        count2++;
        if (count2==100){
            $(this).html(
                "Really!??"
            );
        }
    });
});
